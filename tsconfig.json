{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@ui/*": ["src/ui/*"],
      "@hooks/*"  : ["src/ui/hooks/*"  ],
      "@stores/*" : ["src/ui/stores/*" ],
      "@styles/*" : ["src/ui/styles/*" ],
      "@layouts/*": ["src/ui/layouts/*"],
      "@components/*": ["src/ui/components/*"],
      "@/*": [ "./src/*" ]
    }
  }
}